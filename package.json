{
  "name": "react-fluentui-emoji",
  "version": "0.0.0-beta.0.0.2",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "Mohammad Abu Mattar",
    "email": "mohammad.khaled@outlook.com",
    "url": "https://mkabumattar.github.io/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MKAbuMattar/react-fluentui-emoji.git"
  },
  "bugs": {
    "url": "https://github.com/MKAbuMattar/react-fluentui-emoji/issues"
  },
  "homepage": "",
  "scripts": {
    "lint:fix": "eslint --ignore-path .eslintignore \"src/**/*.ts\" --fix",
    "lint:check": "eslint --ignore-path .eslintignore \"src/**/*.ts\"",
    "prettier:fix": "prettier -w ./src --ignore-unknown --no-error-on-unmatched-pattern",
    "prettier:check": "prettier --check ./src --no-error-on-unmatched-pattern",
    "check": "yarn lint:check && yarn prettier:check",
    "fix": "yarn lint:fix && yarn prettier:fix",
    "clean:fluentui-emoji": "rm -rf fluentui-emoji",
    "clean:svg": "rm -rf svg",
    "clean:lib": "rm -rf lib",
    "clean:svg:cloneup": "rm -rf \"./svg/boy-high contrast-default.svg\"",
    "build:config:clone": "yarn clean:fluentui-emoji && node -r esbuild-runner/register src/config/gitclone.config.ts",
    "build:script:svg": "yarn clean:svg && node -r esbuild-runner/register src/script/svg.script.ts && yarn clean:svg:cloneup",
    "build:config:recursive": "yarn clean:svg:cloneup && node -r esbuild-runner/register src/config/build.config.ts",
    "build:script:jsx": "yarn build:script:svg && yarn build:config:recursive && node -r esbuild-runner/register src/script/jsx.script.ts",
    "build:lib": "yarn clean:lib && babel build --out-dir lib --copy-files",
    "build": "yarn build:script:jsx && yarn build:lib",
    "publish:lib:latest": "npm publish",
    "publish:lib:beta": "npm publish --tag beta",
    "publish:lib:canary": "npm publish --tag canary"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "7.18.10",
    "@babel/plugin-syntax-jsx": "7.18.6",
    "@babel/plugin-transform-parameters": "7.18.8",
    "@babel/preset-react": "7.18.6",
    "@types/fs-extra": "9.0.13",
    "@types/node": "18.7.1",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "babel-preset-minify": "0.5.2",
    "degit": "2.8.4",
    "esbuild": "0.15.1",
    "esbuild-runner": "2.2.1",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "fs-extra": "10.1.0",
    "jsdom": "20.0.0",
    "prettier": "2.7.1",
    "react": "18.2.0",
    "recursive-directory": "0.0.0-rc-0.0.4",
    "svg-to-jsx": "1.0.4",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "walkdir": "0.4.1"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.js",
      "types": "./index.d.ts"
    },
    "./lib/icons/*": {
      "require": "./lib/icons/*.js",
      "import": "./lib/icons/*.js",
      "types": "./lib/icons/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "/lib",
    "/index.d.ts"
  ],
  "support": {
    "backing": {
      "npm-funding": true
    }
  }
}
